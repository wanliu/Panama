<!-- 竞价 -->
<%= simple_form_for @activity, :url => activities_auction_index_path, :html => { class: 'form-horizontal' } do |f| %>
  <%= f.input :product %>
  <%= f.input :price %>
  <div class="control-group">
    <label class="control-label" for="inputProduct">产品</label>
    <div class="controls">
		<div class="input-append">
		  <input class="span2" id="inputProduct" placeholder="Product" type="text">
		  <div class="btn-group">
		    <button class="btn dropdown-toggle" data-toggle="dropdown">
		      选择
		      <span class="caret"></span>
		    </button>
<!-- 		    <ul class="dropdown-menu">
		      ...
		    </ul>
 -->		  </div>
		</div>
    </div>
  </div>
  <div class="control-group">
    <label class="control-label" for="inputPrice">图片</label>
    <div class="controls">
<img data-src="holder.js/140x140" class="img-rounded" alt="140x140" style="width: 140px; height: 140px;" src="data:image/png;base64,">
<img data-src="holder.js/140x140" class="img-rounded" alt="140x140" style="width: 140px; height: 140px;" src="data:image/png;base64,">
<img data-src="holder.js/140x140" class="img-rounded" alt="140x140" style="width: 140px; height: 140px;" src="data:image/png;base64,">
    </div>
  </div>

  <div class="control-group">
    <label class="control-label" for="inputPrice">价格</label>
    <div class="controls">
	  <input class="span2" id="inputPrice" placeholder="价格" type="text">
    </div>
  </div>

  <div class="control-group">
    <label class="control-label" for="inputPrice">&nbsp;</label>
    <div class="controls">
		  <div id="price_slider"></div>
		  <script type="text/javascript">
		  require(['jquery'], function($){
			    $( "#price_slider" ).slider();
		  });
		  </script>
    </div>
  </div>


<% end %>