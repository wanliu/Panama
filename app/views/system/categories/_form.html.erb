<table class = 'index_table' >
	<thead>
		<tr>
			<th>Name</th>
			<th>Title</th>
			<th>Property Type</th>
			<th>操作</th>
		</tr>
	</thead>
	<tbody>
		<% properties.each do |property| %>
			<tr>
				<td> <%= property.name %> </td>
				<td> <%= property.title %> </td>
				<td> <%= property.property_type %></td>
				<td>
					<%#= form_for property, url: delete_relation_system_category_path(category) do |f| %>
						<%#= f.hidden_field :id %>
						<%#= f.submit '删除'%>
					<%# end %>
					<span>
						<%= link_to "详细", system_property_path(property) %>
					</span>
					<span style='margin-left:20px;'>
						<%= link_to "items", items_system_property_path(property) %>
					</span>
				</td>
			</tr>
		<% end %>
	</tbody>
</table>