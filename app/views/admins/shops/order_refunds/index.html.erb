<% admin_active_section :order_refunds %>
<% @refunds.each do |refund| %>
  <%= render "context", :refund => refund %>
<% end %>
<script type='text/javascript'>
  $(".transaction").each(function(i, elem){
    new ShopOrderRefundCard({
      el: elem,
      realtime: {
        url: '<%= faye_server_uri %>',
        token: '<%= token %>'
      }
    })
  })
</script>