<div class="container-fluid">
  <div class="row-fluid">
    <div class="span2">
      退货原因
    </div>
    <div class="span4">
      <%= refund.order_reason.name %>
    </div>
    <div class="span2">
      发货单号
    </div>
    <div class="span4">
      <%= refund.delivery_code %>
    </div>
  </div>

  <div class="row-fluid">
    <div class="span2">
      退货说明
    </div>
    <div class="span10">
      <%= refund.decription %>
    </div>
  </div>
</div>
<%=render "products", :refund => refund %>